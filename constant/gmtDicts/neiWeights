	[neiWeights rawValues -1.0 1.0 0.1 "$GMTU/colours/red_white_blue.cpt"]
	[none mesh "gray"]
	[neiWeights rawBoundaryValues -1.0 1.0 0.1 "$GMTU/colours/red_white_blue.cpt"]
);

